{
    "manifest_version": 3,
    "version": "1.1",
    "name": "URL Storage",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "web_accessible_resources": [{
        "matches": ["<all_urls>"],
        "resources": ["icon.png"]
    }],
    "permissions": [
        "tabs"
    ]
}